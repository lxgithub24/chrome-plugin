{
  "manifest_version": 2,
  "name": "ymmm",
  "version": "0.0.1",
  "description": "ymm",
  "homepage_url": "ymm",
  "icons": {
    "16": "icon/logo128.png",
    "48": "icon/icon48.png",
    "128": "icon/logo128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "ymm"
  },
  "permissions": [
    "background",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}